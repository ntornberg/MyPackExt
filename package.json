{
  "name": "my_pack_extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:debug": "tsc -b && vite build --minify=false --sourcemap=inline",
    "build:prod": "node scripts/build-config.js production && tsc -b && vite build --mode production ",
    "build:staging": "node scripts/build-config.js staging && tsc -b && vite build --mode staging ",
    "build:analyze": "tsc -b && vite build --mode production --analyze && vite build",
    "setup:env": "node scripts/build-config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "node -e \"const fs = require('fs'); if (fs.existsSync('dist')) fs.rmSync('dist', {recursive: true, force: true})\"",
    "prebuild:prod": "npm run clean",
    "postbuild:prod": "npm run package",
    "package": "node -e \"const fs = require('fs'); const path = require('path'); const archiver = require('archiver'); const output = fs.createWriteStream('dist.zip'); const archive = archiver('zip'); output.on('close', () => console.log('Package created: dist.zip')); archive.on('error', (err) => { throw err; }); archive.pipe(output); archive.directory('dist/', false); archive.finalize();\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/lab": "^7.0.0-beta.12",
    "@mui/material": "^7.1.0",
    "@mui/x-charts": "^8.3.0",
    "@mui/x-data-grid": "^8.3.1",
    "@mui/x-date-pickers": "^8.3.1",
    "@mui/x-tree-view": "^8.3.1",
    "@syncfusion/ej2-base": "^29.2.4",
    "@syncfusion/ej2-data": "^29.2.4",
    "@syncfusion/ej2-react-buttons": "^29.2.5",
    "@syncfusion/ej2-react-calendars": "^29.2.5",
    "@syncfusion/ej2-react-dropdowns": "^29.2.5",
    "@syncfusion/ej2-react-inputs": "^29.2.5",
    "@syncfusion/ej2-react-popups": "^29.2.4",
    "@syncfusion/ej2-react-schedule": "^29.2.5",
    "@syncfusion/ej2-schedule": "^29.2.5",
    "@tanstack/react-virtual": "^3.13.9",
    "cheerio": "^1.0.0",
    "devextreme-react": "^24.2.7",
    "jsonpath-plus": "^10.3.0",
    "path": "^0.12.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-gauge-component": "^1.2.64",
    "react-window": "^1.8.11",
    "recharts": "^2.15.3",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.25.9",
    "@babel/plugin-transform-react-inline-elements": "^7.25.9",
    "@crxjs/vite-plugin": "^2.0.0-beta.32",
    "@eslint/js": "^9.26.0",
    "@types/chrome": "^0.0.320",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-window": "^1.8.8",
    "@types/requirejs": "^2.1.37",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "archiver": "^7.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "javascript-obfuscator": "^4.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.36.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-javascript-obfuscator": "^3.1.0"
  }
}
